{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/rifas",
      "dest": "/api/rifas.ts"
    },
    {
      "src": "/api/rifas",
      "dest": "/api/rifas.ts"
    }
  ]
} 